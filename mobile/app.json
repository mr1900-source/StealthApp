{
  "expo": {
    "name": "Drift",
    "slug": "drift",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "drift",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.drift.app"
    },
    "android": {
      "package": "com.drift.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
